<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jinjiang.mapper.IEmployeeMapper">
	<resultMap type="com.jinjiang.domain.Employee" id="employeeResult">
		<result column="empno" property="empNo"/>
		<result column="name" property="name"/>
		<result column="hireday" property="hireDay"/>
		<result column="salary" property="salary"/>
	</resultMap>
	
	<select id="findNameByEmpNo" parameterType="String" resultType="String">
		SELECT  name FROM employee t where t.empno = #{empno}
	</select>
	
	<select id="findAll" resultMap="employeeResult">
		SELECT empno, name, hireday, salary FROM employee
	</select>
</mapper>